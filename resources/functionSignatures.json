{
  "_schemaVersion": "0.1.0",
  "GeoDomain": {
    "inputs": [
      {
        "name": "domain",
        "kind": "required",
        "type": [["char"], ["string"], ["cell"]],
        "purpose": "domain name"
      },
      {
        "name": "Upscale",
        "kind": "ordered",
        "type": [["numeric", "scalar", ">0"], ["numel=0"]],
        "purpose": "factor to increase resolution of domain"
      },
      {
        "name": "Buffer",
        "kind": "ordered",
        "type": [["numeric", "scalar", "real", ">=0"], ["numel=0"]],
        "purpose": "buffer [°] to add around domain"
      },
      {
        "name": "Latlim",
        "kind": "ordered",
        "type": [["numeric", "numel=2"], ["char", "choices={'default'}"], ["string", "choices={'default'}"], ["numel=0"]],
        "purpose": "latitude limits [°] of domain"
      },
      {
        "name": "MoreBuffers",
        "kind": "ordered",
        "type": [["cell"], ["char", "string", "choices={'default'}"]],
        "purpose": "additional buffers add around specific domains"
      },
      {
        "name": "NearBy",
        "kind": "ordered",
        "type": ["logical", "scalar"],
        "purpose": "for Greenland, whether to include nearby regions"
      },
      {
        "name": "DefaultParams",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "use default parameters"
      }
    ]
  },
  "kernelcp_new": {
    "inputs": [
      {
        "name": "L",
        "kind": "required",
        "type": ["numeric", "integer", "scalar", ">0"],
        "purpose": "maximum SH degree/order"
      },
      {
        "name": "Domain",
        "kind": "required",
        "type": [["char"], ["string"], ["cell"], ["GeoDomain"], ["numeric", "ncols=2", "real"]],
        "purpose": "type of region"
      },
      {
        "name": "ForceNew",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to force recomputation"
      },
      {
        "name": "SaveData",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to save computed data"
      },
      {
        "name": "BeQuiet",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to suppress output messages"
      }
    ]
  },
  "grace2slept_new": {
    "inputs": [
      {
        "name": "product",
        "kind": "required",
        "type": ["cell", "numel=3"],
        "purpose": "code of the GRACE product"
      },
      {
        "name": "domain",
        "kind": "required",
        "type": [["char"], ["string", "numel=1"], ["cell"], ["GeoDomain"], ["numeric", "ncols=2", "real"]],
        "purpose": "region of interest"
      },
      {
        "name": "L",
        "kind": "required",
        "type": ["numeric", "integer", "scalar", ">0"],
        "purpose": "maximum spherical harmonic degree/order"
      },
      {
        "name": "Truncation",
        "kind": "namevalue",
        "type": [["numeric", "integer", "scalar", ">0"]],
        "purpose": "number of Slepian functions"
      },
      {
        "name": "Unit",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'SD','POT'}"],
        "purpose": "unit of output"
      },
      {
        "name": "TimeRange",
        "kind": "namevalue",
        "type": [
          ["numeric", "numel=2"],
          ["datetime", "numel=2"]
        ],
        "purpose": "time range of data to read"
      },
      {
        "name": "Deg1Correction",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to apply degree 1 correction"
      },
      {
        "name": "C20Correction",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to apply C20 correction"
      },
      {
        "name": "C30Correction",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to apply C30 correction"
      },
      {
        "name": "RecomputeDegree1",
        "kind": "namevalue",
        "type": [["logical", "scalar"], ["cell"]],
        "purpose": "specs to recompute degree 1 coefficients"
      },
      {
        "name": "ForceNew",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "force reprocess data"
      },
      {
        "name": "SaveData",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "save processed data"
      },
      {
        "name": "BeQuiet",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "suppress output messages"
      },
      {
        "name": "TimeFormat",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'datetime','datenum'}"],
        "purpose": "Type of the time vector"
      }
    ]
  },
  "grace2slept_new": {
    "inputs": [
      {
        "name": "product",
        "kind": "required",
        "type": ["cell", "numel=3", "@(product) ismember(product{1}, {'JPL','CSR','GFZ'})"],
        "purpose": "name of the GRACE product"
      },
      {
        "name": "radius",
        "kind": "required",
        "type": ["numeric", "scalar", ">0"],
        "purpose": "radius of the cap"
      },
      {
        "name": "L",
        "kind": "required",
        "type": ["numeric", "integer", "scalar", ">0"],
        "purpose": "maximum spherical harmonic degree/order"
      },
      {
        "name": "phi",
        "kind": "ordered",
        "type": ["numeric", "scalar", "real"]
      },
      {
        "name": "theta",
        "kind": "ordered",
        "type": ["numeric", "scalar", "real"]
      },
      {
        "name": "omega",
        "kind": "ordered",
        "type": ["numeric", "scalar", "real"]
      },
      {
        "name": "Truncation",
        "kind": "namevalue",
        "type": [["numeric", "integer", "scalar", ">0"]],
        "purpose": "number of Slepian functions"
      },
      {
        "name": "Unit",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'SD','POT'}"],
        "purpose": "unit of output"
      },
      {
        "name": "TimeRange",
        "kind": "namevalue",
        "type": [
          ["numeric", "numel=2"],
          ["datetime", "numel=2"]
        ],
        "purpose": "time range of data to read"
      },
      {
        "name": "Deg1Correction",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to apply degree 1 correction"
      },
      {
        "name": "C20Correction",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to apply C20 correction"
      },
      {
        "name": "C30Correction",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "whether to apply C30 correction"
      },
      {
        "name": "ForceNew",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "force reprocess data"
      },
      {
        "name": "SaveData",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "save processed data"
      },
      {
        "name": "BeQuiet",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "suppress output messages"
      },
      {
        "name": "TimeFormat",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'datetime','datenum'}"],
        "purpose": "Type of the time vector"
      },
      {
        "name": "RecomputeDegree1",
        "kind": "namevalue",
        "type": [["logical", "scalar"], ["cell"]],
        "purpose": "specs to recompute degree 1 coefficients"
      }
    ]
  },
  "ssh2lonlatt": {
    "inputs": [
      {
        "name": "product",
        "kind": "ordered",
        "type": ["char", "string", "choices={'MEaSUREs'}"],
        "purpose": "name of the SSH product"
      },
      {
        "name": "timestep",
        "kind": "ordered",
        "type": [
          ["duration", ["numeric", ">0"]],
          ["char", "string", "choices={'midmonth'}"]
        ],
        "purpose": "interpolation time step"
      },
      {
        "name": "meshsize",
        "kind": "ordered",
        "type": ["numeric"],
        "purpose": "interpolation time step"
      },
      {
        "name": "timelim",
        "kind": "ordered",
        "type": ["numeric", "datetime"],
        "purpose": "time range of data to read"
      },
      {
        "name": "LonOrigin",
        "kind": "namevalue",
        "type": ["numeric", ">0"],
        "purpose": "longitude centre of output grid"
      },
      {
        "name": "Interpolation",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'linear','nearest','spline','pchio'}"],
        "purpose": "longitude centre of output grid"
      },
      {
        "name": "TimeFormat",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'datetime','datenum'}"],
        "purpose": "Type of the time vector"
      },
      {
        "name": "Unit",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'m','mm'}"],
        "purpose": "unit of output"
      },
      {
        "name": "ForceNew",
        "kind": "namevalue",
        "type": ["logical"],
        "purpose": "force reprocess data"
      },
      {
        "name": "BeQuiet",
        "kind": "namevalue",
        "type": ["logical"],
        "purpose": "suppress output messages"
      },
      {
        "name": "SaveData",
        "kind": "namevalue",
        "type": ["logical"],
        "purpose": "save processed data"
      }
    ]
  },
  "gia2plmt": {
    "inputs": [
      {
        "name": "time",
        "kind": "ordered",
        "type": ["duration", "numeric", "datetime"],
        "purpose": "time stamp of GIA"
      },
      {
        "name": "model",
        "kind": "ordered",
        "type": ["char", "string", "choices={'Caron18', 'Caron19', 'A13', 'ICE6GD', 'Paulson07}"],
        "purpose": "name of the GIA model"
      },
      {
        "name": "L",
        "kind": "ordered",
        "type": ["numeric"],
        "purpose": "degree/order of spherical harmonics"
      },
      {
        "name": "Unit",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'SD','POT'}"],
        "purpose": "Unit of output"
      },
      {
        "name": "OutputFormat",
        "kind": "namevalue",
        "type": ["char", "string", "choices={'timefirst','traditional'}"],
        "purpose": "dimension order of output"
      },
      {
        "name": "BeQuiet",
        "kind": "namevalue",
        "type": ["logical"],
        "purpose": "suppress output messages"
      }
    ]
  }
}
