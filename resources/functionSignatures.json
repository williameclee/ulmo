{
  "_schemaVersion": "0.1.0",
  "_typedefs": {
    "GRACE_product": {
      "purpose": "GRACE product",
      "type": "cell",
      "elements": [
        {
          "type": ["char", "choices={'CSR', 'JPL', 'GFZ'}"],
          "purpose": "data centre"
        },
        {
          "type": ["char", "choices={'RL06'}"],
          "purpose": "release level"
        },
        {
          "type": ["numeric", "choices={60,96}"],
          "purpose": "SH d/o"
        }
      ]
    }
  },
  "GeoDomain": {
    "purpose": "Class representing a geographical domain",
    "inputs": [
      {
        "name": "domain",
        "kind": "required",
        "type": [["char"], ["cell"]],
        "purpose": "name"
      },
      {
        "name": "Upscale",
        "kind": "ordered",
        "type": [["numeric", "scalar", ">0"], ["numel=0"]],
        "purpose": "factor to increase resolution of domain"
      },
      {
        "name": "Buffer",
        "kind": "ordered",
        "type": [["numeric", "scalar", "real", ">=0"], ["numel=0"]],
        "purpose": "buffer around domain"
      },
      {
        "name": "Latlim",
        "kind": "ordered",
        "type": [
          ["numeric", "numel=2"],
          ["char", "choices={'default'}"]
        ],
        "purpose": "latitude limits"
      },
      {
        "name": "MoreBuffers",
        "kind": "ordered",
        "type": [["cell"], ["char", "choices={'default'}"]],
        "purpose": "additional buffers"
      },
      {
        "name": "NearBy",
        "kind": "ordered",
        "type": ["logical", "scalar"],
        "purpose": "for Greenland, whether to include nearby regions"
      },
      {
        "name": "DefaultParams",
        "kind": "namevalue",
        "type": ["logical", "scalar"],
        "purpose": "use default parameters"
      }
    ]
  },
  "kernelcp_new": {
